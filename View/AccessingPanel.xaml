<UserControl x:Class="DailyCheck.View.ConsolePanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:uc="clr-namespace:DailyCheck.View"
             xmlns:local="clr-namespace:DailyCheck.View"
             mc:Ignorable="d" 
             d:DesignHeight="360" d:DesignWidth="300">
    <Border Grid.Row="1"
        Margin="10, 5"
        Padding="5,2"
        BorderThickness="1"
        BorderBrush="White"
        CornerRadius="5"
        Background="#30FFFFFF"
        >
        <ScrollViewer VerticalScrollBarVisibility="Auto" x:Name="Scroller">
            <StackPanel x:Name="ElementsPanel">
            </StackPanel>
        </ScrollViewer>
    </Border>
</UserControl>
